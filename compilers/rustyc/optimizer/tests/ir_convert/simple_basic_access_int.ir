function main () {
block1:
	t1 = stackalloc i32, size 4, align 8
	store i32 0 [t1, 0]
	t2 = stackalloc i32, size 4, align 8
	load i32 t3 [t1, 0]
	store i32 t3 [t2, 0]
	load i32 t4 [t2, 0]
	load i32 t5 [t1, 0]
	t6 = add t4 t5
	store i32 t6 [t2, 0]
	load i32 t7 [t1, 0]
	load i32 t8 [t1, 0]
	t9 = add t7 t8
	load i32 t10 [t2, 0]
	t11 = add t9 t10
	store i32 t11 [t2, 0]
}
